  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "yarn develop",
    "develop": "gatsby develop -p 1234",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint src posts",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
